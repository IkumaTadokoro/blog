{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm run corepack",
    "corepack": "corepack enable pnpm",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "rm -rf dist && astro build",
    "deploy": "pnpm build && wrangler pages deploy dist",
    "preview": "wrangler pages dev ./dist",
    "preview:local": "astro preview",
    "check": "pnpm check:astro && pnpm check:biome",
    "check:astro": "astro check",
    "check:biome": "biome check .",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "astro": "astro"
  },
  "dependencies": {
    "@ascorbic/feed-loader": "1.0.4",
    "@astrojs/check": "0.9.4",
    "@astrojs/markdown-remark": "6.3.1",
    "@astrojs/mdx": "4.2.5",
    "@astrojs/react": "4.2.5",
    "@astrojs/rss": "4.0.11",
    "@expressive-code/plugin-line-numbers": "0.41.2",
    "@extractus/feed-extractor": "7.1.4",
    "@extractus/oembed-extractor": "4.0.7",
    "@fontsource-variable/geist": "5.2.5",
    "@fontsource-variable/geist-mono": "5.2.5",
    "@fontsource-variable/lexend": "5.2.6",
    "@fontsource-variable/montserrat": "5.2.5",
    "@fontsource-variable/noto-sans-jp": "5.2.5",
    "@iconify-json/lucide": "1.2.39",
    "@tailwindcss/vite": "4.1.4",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "astro": "5.7.5",
    "astro-expressive-code": "0.41.2",
    "astro-icon": "1.1.5",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "ofetch": "1.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
