    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.2",
    "@types/sanitize-html": "2.15.0",
    "@vitest/browser": "3.1.2",
    "@vitest/ui": "3.1.2",
