{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm run corepack",
    "corepack": "corepack enable pnpm",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "rm -rf dist && astro build",
    "deploy": "pnpm build && wrangler deploy",
    "preview": "wrangler dev",
