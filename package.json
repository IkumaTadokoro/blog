{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm run corepack",
    "corepack": "corepack enable pnpm",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "rm -rf dist && astro build && pnpm build:pagefind",
    "build:pagefind": "pagefind --site dist",
    "deploy": "pnpm build && wrangler pages deploy dist",
    "preview": "wrangler pages dev ./dist",
    "preview:local": "astro preview",
    "check": "pnpm check:astro && pnpm check:biome",
    "check:astro": "astro check",
    "check:biome": "biome check .",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "0.8.3",
    "@astrojs/react": "3.6.1",
    "@astrojs/rss": "4.0.7",
    "@astrojs/tailwind": "5.1.0",
    "@expressive-code/plugin-line-numbers": "0.35.3",
    "@fontsource-variable/lexend": "5.0.20",
    "@fontsource-variable/montserrat": "5.0.19",
    "@iconify-json/lucide": "1.1.203",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "astro": "4.12.3",
    "astro-expressive-code": "0.35.3",
    "astro-icon": "1.1.0",
    "astro-pagefind": "1.6.0",
    "clsx": "2.1.1",
    "pagefind": "1.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "rehype-slug": "6.0.0",
    "sanitize-html": "2.13.0",
    "tailwind-merge": "2.4.0",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.6.0",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@playwright/test": "1.45.3",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.4.8",
    "@types/node": "20.14.13",
    "@types/sanitize-html": "2.11.0",
    "@vitest/browser": "2.0.4",
    "@vitest/ui": "2.0.4",
    "happy-dom": "14.12.3",
    "jsdom": "24.1.1",
    "lefthook": "1.7.11",
    "vitest": "2.0.4",
    "wrangler": "3.66.0"
  }
}
